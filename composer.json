{
    "name": "jiny/html",
    "description": "jiny object html",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3|^8.0",
        "livewire/livewire": "^2.7"

    },
    "autoload": {
        "psr-4": {
            "Jiny\\Html\\":"src/"
        },
        "files" : [
            "src/Helpers/Helper.php",
            "src/Helpers/Factory.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Html\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\Html\\JinyHtmlServiceProvider"
            ]
        }
    }

}
